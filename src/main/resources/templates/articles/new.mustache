{{>layout/header}}
    <div class="container">
        <div class="jumbotron">
            <h1>New Article</h1>
        </div>
    </div>

    <div class="container">
        <form method="post" action="/article/new">
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">제목</label>
                <input type="text" class="form-control col-sm-10" name="title">
            </div>
            <div class="form-group row">
                <label class="col-sm-2 col-form-label">내용</label>
                <input type="text" class="form-control col-sm-10" name="content">
            </div>
            <button class="btn btn-primary">제출</button>
            <a class="btn btn-danger" href = "/">취소</a>
        </form>

    </div>



{{>layout/footer}}